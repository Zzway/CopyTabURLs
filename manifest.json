   "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
   "manifest_version": 2,
   "name": "Copy Tab URLs",
   "options_page": "options.html",
   "permissions": [ "tabs"],
   "version": "0.9"
}
